        "import math\n",
        "import matplotlib.pyplot as plt\n",
        "def area_circulo(radio):\n",
        "    \"\"\"Calcula el área de un círculo dado su radio.\"\"\"\n",
        "    return math.pi * (radio ** 2)\n",
        "\n",
        "def volumen_cilindro(radio, altura):\n",
        "    \"\"\"Calcula el volumen de un cilindro usando el área del círculo.\"\"\"\n",
        "    area = area_circulo(radio)\n",
        "    return area * altura\n",
        "# Ejemplo de uso\n",
        "radio = 5\n",
        "altura = 10\n",
        "area = area_circulo(radio)\n",
        "volumen = volumen_cilindro(radio, altura)\n",
        "print(\"Área del círculo:\", area)\n",
        "print(\"Volumen del cilindro:\", volumen)\n",
        "# Visualización\n",
        "plt.figure(figsize=(8, 4))\n",
        "# Gráfico del área\n",
        "plt.subplot(1, 2, 1)\n",
        "plt.pie([area, 100-area], labels=['Área', 'Resto'], autopct='%1.1f%%', startangle=90)\n",
        "plt.title('Área del Círculo')\n",
        "# Gráfico del volumen\n",
        "plt.subplot(1, 2, 2)\n",
        "plt.bar(['Cilindro'], [volumen], color='lightblue')\n",
        "plt.title('Volumen del Cilindro')\n",
        "plt.ylabel('Volumen')\n",
        "\n",
        "plt.tight_layout()\n",
        "plt.show()\n",
        "\n",
        "\n"
      ]
