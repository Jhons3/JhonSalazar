        "import math\n",
        "import matplotlib.pyplot as plt\n",
        "def factorial(n):\n",
        "    \"\"\"Calcula el factorial de un número entero positivo.\"\"\"\n",
        "    if n < 0:\n",
        "        return \"El número debe ser positivo.\"\n",
        "    elif n == 0 or n == 1:\n",
        "        return 1\n",
        "    else:\n",
        "        resultado = 1\n",
        "        for i in range(2, n + 1):\n",
        "            resultado *= i\n",
        "        return resultado\n",
        "# Ejemplo de uso\n",
        "numero = 5\n",
        "resultado_factorial = factorial(numero)\n",
        "print(\"El factorial de\", numero, \"es:\", resultado_factorial)\n",
        "# Visualización\n",
        "n_values = list(range(0, 11))  # Factoriales de 0 a 10\n",
        "factorial_values = [factorial(n) for n in n_values]\n",
        "plt.figure(figsize=(8, 4))\n",
        "plt.plot(n_values, factorial_values, marker='o')\n",
        "plt.title('Factorial de N')\n",
        "plt.xlabel('N')\n",
        "plt.ylabel('Factorial(N)')\n",
        "plt.xticks(n_values)\n",
        "plt.grid()\n",
        "plt.show()\n",
        "\n"
      ],
