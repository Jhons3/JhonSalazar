      "source": [
        "import math\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "def total_factura(cantidad_sin_iva, porcentaje_iva=12):\n",
        "    \"\"\"Calcula el total de una factura tras aplicarle el IVA.\"\"\"\n",
        "    iva = cantidad_sin_iva * (porcentaje_iva / 100)\n",
        "    total = cantidad_sin_iva + iva\n",
        "    return total\n",
        "\n",
        "# Ejemplo de uso\n",
        "cantidad = 100\n",
        "total_con_iva_12 = total_factura(cantidad)\n",
        "total_con_iva_20 = total_factura(cantidad, 20)\n",
        "\n",
        "print(\"Total de la factura (sin IVA):\", total_con_iva_12)\n",
        "print(\"Total de la factura (con IVA del 20%):\", total_con_iva_20)\n",
        "\n",
        "# Visualización\n",
        "labels = ['Sin IVA (12%)', 'Con IVA (20%)']\n",
        "totales = [total_con_iva_12, total_con_iva_20]\n",
        "\n",
        "plt.figure(figsize=(6, 4))\n",
        "plt.bar(labels, totales, color=['orange', 'green'])\n",
        "plt.title('Total de la Factura con IVA')\n",
        "plt.ylabel('Total')\n",
        "plt.show()\n"
      ],
