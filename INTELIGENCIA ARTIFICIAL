      "cell_type": "code",
      "source": [
        "# Importar las librerías necesarias\n",
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# Datos de estatura y peso\n",
        "estatura = np.array([120, 110, 140, 130, 125, 115])\n",
        "peso = np.array([25, 30, 35, 25, 20, 20])\n",
        "\n",
        "# Calcular el coeficiente de correlación de Pearson\n",
        "coef_pearson = np.corrcoef(estatura, peso)[0, 1]\n",
        "print(f\"Coeficiente de correlación de Pearson: {coef_pearson}\")\n",
        "\n",
        "# Crear el gráfico de dispersión\n",
        "plt.scatter(estatura, peso, color='b', label='Datos')\n",
        "\n",
        "# Añadir título y etiquetas\n",
        "plt.title('Relación entre Estatura y Peso')\n",
        "plt.xlabel('Estatura (cm)')\n",
        "plt.ylabel('Peso (kg)')\n",
        "\n",
        "# Mostrar la gráfica\n",
        "plt.legend()\n",
        "plt.grid(True)\n",
        "plt.show()\n"
      ],
